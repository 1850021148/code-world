/* components/detailPage/navBar/navBar.wxss */
.list {
  display: flex;
  position: sticky;
  top: 0;
  z-index: 100;
  width: 100%;
  background: white;
  box-shadow: 0 1px 3px lightgray;
}
.link {
  padding: 0;
  font-size: 12px;
  height: 40px;
  line-height: 40px;
  font-weight: normal;
  text-align: center;
}
.link.active {
  color: rgb(230,208,153);
}